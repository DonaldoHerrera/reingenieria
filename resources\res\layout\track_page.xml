<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/player_track_page" android:layout_width="match_parent" android:layout_height="match_parent">
    <com.soundcloud.android.playback.ui.view.PlayerTrackArtworkView android:id="@+id/track_page_artwork" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <View android:background="@color/artwork_overlay" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <View android:background="@drawable/player_gradient_top" android:layout_width="match_parent" android:layout_height="@dimen/player_gradient_height" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toBottomOf="@+id/upsell_container"/>
    <View android:background="@drawable/player_gradient_bottom" android:layout_width="match_parent" android:layout_height="@dimen/player_gradient_height" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toRightOf="0"/>
    <com.soundcloud.android.playback.ui.view.PlayerUpsellView android:id="@+id/upsell_container" android:background="@drawable/go_gradient" android:paddingLeft="14dp" android:paddingTop="11dp" android:paddingRight="14dp" android:paddingBottom="14dp" android:focusable="true" android:visibility="gone" android:clickable="true" android:layout_width="match_parent" android:layout_height="@dimen/player_upsell_height" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toTopOf="0"/>
    <com.soundcloud.android.view.ThemeableMediaRouteButton android:id="@+id/media_route_button" android:visibility="gone" android:layout_width="72dp" android:layout_height="72dp" app:layout_constraintRight_toLeftOf="@+id/player_close_indicator" app:layout_constraintTop_toBottomOf="@+id/upsell_container" style="@style/PlayerMediaRouteButton"/>
    <ImageButton android:id="@+id/player_close_indicator" android:contentDescription="@string/accessibility_close" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toBottomOf="@+id/upsell_container" style="@style/PlayerCloseBase.PlayerClose"/>
    <View android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="@integer/waveform_ratio_top" app:layout_constraintBottom_toTopOf="@+id/track_page_waveform" app:layout_constraintLeft_toLeftOf="0"/>
    <com.soundcloud.android.player.ui.waveform.WaveformView android:id="@+id/track_page_waveform" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent" style="@style/Waveform"/>
    <com.soundcloud.android.playback.ui.view.TimestampView android:id="@+id/timestamp" android:layout_width="match_parent" android:layout_height="match_parent" app:layout_constraintBottom_toTopOf="@+id/track_page_more"/>
    <com.soundcloud.android.view.customfontviews.CustomFontToggleButton android:id="@+id/track_page_like" android:layout_height="@dimen/player_engagement" app:layout_constraintBottom_toBottomOf="@+id/track_page_more" app:layout_constraintRight_toLeftOf="@+id/track_page_comment" style="@style/PlayerLikeBase.PlayerLike"/>
    <ImageButton android:id="@+id/track_page_comment" android:visibility="gone" android:layout_height="@dimen/player_engagement" android:scaleType="center" android:contentDescription="@string/accessibility_comments" app:layout_constraintBottom_toBottomOf="@+id/track_page_more" app:layout_constraintRight_toLeftOf="@+id/track_page_share" style="@style/PlayerComment"/>
    <ImageButton android:id="@+id/track_page_share" android:layout_height="@dimen/player_engagement" android:contentDescription="@string/accessibility_share" app:layout_constraintBottom_toBottomOf="@+id/track_page_more" app:layout_constraintRight_toLeftOf="@+id/play_queue_button" style="@style/PlayerShare"/>
    <ImageButton android:id="@+id/play_queue_button" android:layout_height="@dimen/player_engagement" android:contentDescription="@string/accessibility_play_queue" app:layout_constraintBottom_toBottomOf="@+id/track_page_more" app:layout_constraintRight_toLeftOf="@+id/track_page_more" style="@style/PlayerQueue"/>
    <com.soundcloud.android.view.OverflowAnchorImageButton android:id="@+id/track_page_more" android:layout_height="@dimen/player_engagement" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintRight_toRightOf="0" style="@style/PlayerMore"/>
    <ImageButton android:id="@+id/player_previous" android:layout_centerVertical="true" android:contentDescription="@string/accessibility_previous" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintTop_toTopOf="0" style="@style/PlayerSkip.Previous"/>
    <ImageButton android:id="@+id/player_play" android:contentDescription="@string/accessibility_play" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toTopOf="0" style="@style/PlayerPlay"/>
    <ImageButton android:id="@+id/player_next" android:layout_centerVertical="true" android:contentDescription="@string/accessibility_next" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toTopOf="0" style="@style/PlayerSkip.Next"/>
    <com.soundcloud.android.view.JaggedTextView android:id="@+id/track_page_user" android:layout_marginTop="@dimen/player_title_top" android:layout_marginStart="16dp" android:layout_marginEnd="4dp" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintTop_toBottomOf="@+id/player_close_indicator" style="@style/PlayerUser"/>
    <com.soundcloud.android.view.JaggedTextView android:id="@+id/track_page_title" android:layout_marginTop="3dp" android:layout_below="@+id/track_page_user" app:layout_constraintLeft_toLeftOf="@+id/track_page_user" app:layout_constraintTop_toBottomOf="@+id/track_page_user" style="@style/PlayerTitle"/>
    <com.soundcloud.android.view.JaggedTextView android:id="@+id/track_page_context" app:layout_constraintLeft_toLeftOf="@+id/track_page_user" app:layout_constraintTop_toBottomOf="@+id/track_page_title" style="@style/PlayerContext"/>
    <include android:layout_width="match_parent" android:layout_height="wrap_content" layout="@layout/player_footer_controls"/>
    <com.soundcloud.android.playback.ui.view.MiniplayerProgressView android:id="@+id/player_footer_progress" android:layout_width="match_parent" android:layout_height="@dimen/player_footer_progressbar_height" app:layout_constraintBottom_toBottomOf="@+id/footer_controls" app:layout_constraintLeft_toLeftOf="0"/>
    <ViewStub android:id="@+id/track_page_empty_stub" android:layout="@layout/track_page_empty" android:inflatedId="@+id/track_page_empty" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <ViewStub android:id="@+id/track_page_error_stub" android:layout="@layout/track_page_error" android:inflatedId="@+id/track_page_error" android:layout_width="match_parent" android:layout_height="match_parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>
