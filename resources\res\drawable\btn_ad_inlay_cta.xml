<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/ad_inlay_cta_border_radius"/>
            <stroke android:width="1dp" android:color="@color/charcoal"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/ad_inlay_cta_border_radius"/>
            <stroke android:width="1dp" android:color="@color/teal"/>
        </shape>
    </item>
</selector>
