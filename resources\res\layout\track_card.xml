<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="wrap_content">
    <com.soundcloud.android.view.ParallaxImageView android:id="@+id/image" app:foreground="@drawable/artwork_border" style="@style/Stream.Image.Track"/>
    <com.soundcloud.android.view.JaggedTextView android:id="@+id/creator" android:layout_above="@+id/title" style="@style/Cards.ArtistText"/>
    <com.soundcloud.android.view.JaggedTextView android:id="@+id/title" android:layout_alignBottom="@+id/image" style="@style/Cards.TitleText"/>
    <View android:id="@+id/go_indicator" android:layout_marginTop="@dimen/go_indicator_offset" android:layout_marginRight="@dimen/go_indicator_offset" android:layout_alignTop="@+id/image" android:layout_alignRight="@+id/image" style="@style/GoIndicator"/>
</RelativeLayout>
