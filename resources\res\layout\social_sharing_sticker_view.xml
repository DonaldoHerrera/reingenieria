<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:background="@android:color/transparent" android:layout_width="wrap_content" android:layout_height="wrap_content">
    <LinearLayout android:orientation="vertical" android:background="@drawable/rounded_white_background" android:paddingTop="32px" android:paddingBottom="24px" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="44px" android:paddingStart="24px" android:paddingEnd="24px" android:layout_marginStart="20px">
        <com.soundcloud.android.view.customfontviews.CustomFontTextView android:id="@+id/shareStickerTitle" android:maxLines="2" app:custom_font="@string/interstate_regular" style="@style/SocialSharing.StickerText"/>
        <com.soundcloud.android.view.customfontviews.CustomFontTextView android:id="@+id/shareStickerSubtitle" android:layout_marginTop="4px" android:maxLines="1" app:custom_font="@string/interstate_light" style="@style/SocialSharing.StickerText"/>
    </LinearLayout>
    <ImageView android:id="@+id/shareStickerLogoIm" android:layout_width="132px" android:layout_height="68px" android:src="@drawable/ic_logo_sticker"/>
</FrameLayout>
