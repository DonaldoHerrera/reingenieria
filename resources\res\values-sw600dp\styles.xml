<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Appboy.InAppMessage.Full">
        <item name="android:background">@drawable/com_appboy_inappmessage_modal_background</item>
        <item name="android:padding">0dp</item>
        <item name="android:layout_width">375dp</item>
        <item name="android:layout_height">600dp</item>
        <item name="android:layout_centerInParent">true</item>
    </style>
    <style name="Auth.Container">
        <item name="android:layout_gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:paddingTop">@dimen/auth_container_padding_top</item>
        <item name="android:layout_width">@dimen/auth_social_button_width</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="Widget.Design.TabLayout">
        <item name="tabGravity">1</item>
        <item name="tabMode">1</item>
    </style>
</resources>
